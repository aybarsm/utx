{
    "name": "aybarsm/utx",
    "type": "project",
    "description": "A lightweight PHP CLI of Swiss army knife.",
    "keywords": ["utx", "php", "cli", "tempest"],
    "support": {
        "issues": "https://github.com/aybarsm/utx/issues",
        "source": "https://github.com/aybarsm/utx"
    },
    "authors": [
        {
            "name": "Murat Aybars",
            "email": "murat.aybars@blrm.co"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "tempest/console": "^2.14",
        "tempest/process": "^2.14",
        "tempest/event-bus": "^2.14",
        "tempest/command-bus": "^2.14",
        "tempest/mapper": "^2.14",
        "symfony/finder": "^8.0",
        "tempest/support": "^2.14",
        "tempest/cryptography": "^2.14",
        "tempest/database": "^2.14",
        "opis/closure": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "carthage-software/mago": true
        }
    },
    "require-dev": {
        "swoole/ide-helper": "@dev"
    }
}
