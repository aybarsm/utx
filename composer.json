{
    "name": "aybarsm/helper",
    "type": "project",
    "description": "A lightweight PHP CLI tool for consolidated helpers.",
    "keywords": ["php", "cli", "tempest"],
    "support": {
        "issues": "https://github.com/aybarsm/helper/issues",
        "source": "https://github.com/aybarsm/helper"
    },
    "authors": [
        {
            "name": "Murat Aybars",
            "email": "murat.aybars@blrm.co"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "tempest/console": "^2.13",
        "tempest/process": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "carthage-software/mago": true
        }
    },
    "require-dev": {
        "swoole/ide-helper": "@dev"
    }
}
